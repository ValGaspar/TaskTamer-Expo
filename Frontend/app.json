{
  "expo": {
    "newArchEnabled": true,
    "name": "TaskTamer",
    "scheme": "tasktamer",
    "slug": "tasktamer",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#98B88F",
      "loadingIndicator": "none"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "resizeMode": "contain",
        "backgroundColor": "#98B88F"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#98B88F"
      },
      "splash": {
        "image": "./assets/images/TasktamerLOGO.png",
        "resizeMode": "contain",
        "backgroundColor": "#98B88F"
      }
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-web-browser"
    ]
  }
}
