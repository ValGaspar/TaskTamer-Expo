{
  "expo": {
    "newArchEnabled": true,
    "name": "TaskTamer",
    "scheme": "TASKTAMER",
    "slug": "TaskTamer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/TasktamerLogo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/TasktamerLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#98B88F",
      "loadingIndicator": "none"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/TasktamerLogo.png",
        "resizeMode": "contain",
        "backgroundColor": "#98B88F"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/TasktamerLogo.png",
        "backgroundColor": "#98B88F"
      },
      "splash": {
        "backgroundColor": "#98B88F",
        "resizeMode": "contain"
      }
    },
    "plugins": ["expo-router"]
  }
}